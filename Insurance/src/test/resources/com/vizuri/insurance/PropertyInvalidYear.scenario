<scenario>
  <name>PropertyInvalidYear.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>Property</type>
      <fieldData>
        <field-data>
          <name>yearBuilt</name>
          <value>2016</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$prop</name>
      <isModify>false</isModify>
    </fact-data>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>quote-error-check</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>property</fieldName>
          <expected>yearBuilt</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>message</fieldName>
          <expected>Year Built cannot be greater than current year</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>QuoteMessage</name>
      <anonymous>true</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.vizuri.insurance</packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.insurance.domain.Property</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.insurance.domain.QuoteMessage</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.insurance.domain.QuoteMessageStatus</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>